{
  "functions": {
    "backend/main.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "backend"
    }
  }
}